int lastSectorPos		= 0x4f;
int dpvmFileSizePos		= 0x335;
int dpvmFileIndexPos		= 0x480;
int entryPointPos		= 0x4cd;
int dpvmFileStart		= 0x2600;

(volatile stateful any bootfloppyObj, const char data[], int flags) -> (const char error[]) addSection = {
	bootfloppyImpl impl;
	char err[] = convertObjToImpl(impl, bootfloppyObj, "addSection");
	if (err.csize) return err;

	if (!impl.data.archSet)
		return mkError("arch is not set", "addSection");

	if (!impl.data.outputFileSet)
		return mkError("output file is not set", "addSection");

	if (!impl.data.outputFilePos)
		impl.data.outputFilePos = header.isize << 3;

	writep({}, {}, impl.data.outputFile, data, impl.data.outputFilePos);
	impl.data.outputFilePos += data.csize;

	if (flags == -1) {
		int h[];
		objcopyto(h, header);
		int size = impl.data.outputFilePos - dpvmFileStart, i;
		if (size > 0x8000)
			return mkError("too large file dpvm not supported", "addSection");
		for (i = 0; i <= (size - 1) >> 9; i++) {
			h[dpvmFileIndexPos + i] = (dpvmFileStart >> 9) + i;
		}
		h[lastSectorPos] = (impl.data.outputFilePos - 1) >> 9;
		h[dpvmFileSizePos] = size;
		h[entryPointPos] = impl.data.entryPoint;
		writep({}, {}, impl.data.outputFile, ints2chars(h, 0, h.isize), 0);
		writep({}, {}, impl.data.outputFile, "", 1440 << 10);
	}

	return "";
