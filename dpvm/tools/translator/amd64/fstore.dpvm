/* dpvm byte code translator, amd64 float store; T16.183-T18.397; $DVS:time$ */

#include "../translator_utils.dpvmake"

(volatile stateful asm_instr list[], const char mnem[], const char hex[],
		int paramA, int paramB, int paramC, int paramD, int next) -> ()
	as = asm_add;

(const stateful translator_data data, volatile stateful asm_instr list[],
		int i, int n) -> () {
	n = data.l[4].l[i].i[2] - data.l[0].i[2] - n - 1;

	if (n >= -16 && n <= 15)
		as(list, "fstp [r11+b]", "41 DD 5B 111", 0, n<<3, 0, 0, 0);
	else
		as(list, "fstp [r11+d]", "41 DD 9B 114", 0, n<<3, 0, 0, 0);
	return;
}
