#include "../registry/registry.dpvmake"

(const stateful any regSrc) -> (volatile registryData reg, const char error[]) {
        registryData reg = registryObj.copy(regSrc);
	char error[] = registryObj.check(reg, REGISTRY_COMPILER);
	return (reg, error);
}
