/* converts ints array to chars array; T17.550-T17.550; $DVS:time$ */

(const stateful int src[], int begin, int end) -> (volatile char arr[]) {
	char arr[];
	int i;
	for (i = begin; i < end; i += 1) {
		int n = src[i];
		arr.cpush(n); n >>= 8; 
		arr.cpush(n); n >>= 8; 
		arr.cpush(n); n >>= 8; 
		arr.cpush(n); n >>= 8; 
		arr.cpush(n); n >>= 8; 
		arr.cpush(n); n >>= 8; 
		arr.cpush(n); n >>= 8; 
		arr.cpush(n); 
	}

	return arr;
}