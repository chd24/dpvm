/* find src in dst, return index or -1; T15.543-T17.056; $DVS:time$ */
	
(const stateful char dst[], const stateful char src[],
		const stateful char table[256]) -> (int index) {
	int size = src.csize;
	int bigsize = dst.csize;
	int index;

	for (index = 0; size + index <= bigsize; index = index + 1) {
		int i = -1;
		do {
			i = i + 1;
			if (i == size)
				return index;
		} while (table[src[i]] == table[dst[i + index]]);
	}
	
	return -1;
}
