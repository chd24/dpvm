/* add int32[] subarray to crc32 context; T16.508-T17.641; $DVS:time$ */

(const stateful int ctx[], const stateful int data[], int begin, int end, int crc) -> (int crc) {
	const int f = 0xff;

	while (begin < end) {
		int c = data[begin];

		crc = ctx[(crc ^ c      ) & f] ^ crc >> 8;
		crc = ctx[(crc ^ c >>  8) & f] ^ crc >> 8;
		crc = ctx[(crc ^ c >> 16) & f] ^ crc >> 8;
		crc = ctx[(crc ^ c >> 24) & f] ^ crc >> 8;

		begin = begin + 1;
	}

	return crc;
}
